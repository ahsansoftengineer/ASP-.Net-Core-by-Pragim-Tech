@model IEnumerable<Core_Demo_2.Models.Employee>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>All Employees</title>
</head>
<body>
    <table>
        <thead>
            <tr>
                <td>ID</td>
                <td>Name</td>
                <td>Email</td>
                <td>Department</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Email</td>
                <td>@item.Department</td>
            </tr>
            }
        </tbody>
    </table>
</body>
</html>