    @model IEnumerable<Core_Demo_2.Models.Employee>

    @{
        Layout = "~/Views/Shared/_Layout.cshtml";
        ViewBag.Title = "Employees";
    }
    <table>
        <thead>
            <tr>
                <td>ID</td><td>Name</td><td>Email</td><td>Department</td>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@item.Id</td>
                    <td>@item.Name</td>
                    <td>@item.Email</td>
                    <td>@item.Department</td>
                </tr>
            }
        </tbody>
    </table>
